log"],"sources":["C:\\Users\\Edwaed\\Desktop\\fund-management-app\\src\\views\\StockDetailPage.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <div class=\"box\" :class=\"w\">\r\n      <h1 class=\"section-title\" v-if=\"title\">{{ title }}</h1>\r\n      <div class=\"detail-header\">\r\n        <div class=\"header-left\">\r\n          <div class=\"logo-container\">\r\n            <div class=\"logo\">\r\n              <img :src=\"stockInfo.LogoUrl\" :alt=\"stockInfo.CompanyName\" />\r\n            </div>\r\n            <div class=\"company-info\">\r\n              <h2>\r\n                {{\r\n                  stockInfo.CompanyName.length > 20\r\n                    ? stockInfo.CompanyName.slice(0, 20) + \"...\"\r\n                    : stockInfo.CompanyName\r\n                }}\r\n              </h2>\r\n              <a-button @click=\"toggleWatchlist\">\r\n                <a-icon :type=\"hasStock(symbol) ? 'heart' : 'heart-o'\" />\r\n              </a-button>\r\n            </div>\r\n          </div>\r\n          <p>{{ symbol }}</p>\r\n        </div>\r\n        <div class=\"header-right\">\r\n          <div class=\"stock-info\">\r\n            <h1>â‚¬ {{ Number(chartData.y[0]).toFixed(2) }}</h1>\r\n            <h3\r\n              :class=\"\r\n                stockInfo.PriceChange.includes('-') ? 'stock-down' : 'stock-up'\r\n              \"\r\n            >\r\n              <a-icon\r\n                :type=\"\r\n                  stockInfo.PriceChange.includes('-')\r\n                    ? 'caret-down'\r\n                    : 'caret-up'\r\n                \"\r\n              />\r\n              {{ stockInfo.PriceChange }}\r\n            </h3>\r\n          </div>\r\n          <div class=\"action-buttons\">\r\n            <a-button :disabled=\"getBoughtStocks() < 1\" @click=\"openSell\"\r\n              >Sell</a-button\r\n            >\r\n            <a-button @click=\"openBuy\">Buy</a-button>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <stock-chart :chartData=\"chartData\" />\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { ref, computed } from \"vue\";\r\nimport { useRoute } from \"vue-router\";\r\nimport { Button, Icon } from \"ant-design-vue\";\r\nimport { stocksArr } from \"@/store/stockArr.js\"; // Import your stock data array\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  props: {\r\n    symbol2: String,\r\n    title: String,\r\n    w: String,\r\n  },\r\n  components: {\r\n    aButton: Button,\r\n    aIcon: Icon,\r\n  },\r\n  setup() {\r\n    const route = useRoute();\r\n    const stocksToBuy = ref(0);\r\n    const selectedOption = ref(\"\");\r\n    const amount = ref(0);\r\n\r\n    const stockInfo = computed(() => {\r\n      const selectedStock = stocksArr.find(\r\n        (stock) => stock.Symbol === route.params.symbol\r\n      );\r\n      return selectedStock || {};\r\n    });\r\n\r\n    return {\r\n      symbol: ref(null),\r\n      isDrawerOpen: ref(false),\r\n      chartData: ref({}),\r\n      stockInfo,\r\n      stocksToBuy,\r\n      selectedOption,\r\n      amount,\r\n      isOpen: ref(false),\r\n    };\r\n  },\r\n  async mounted() {\r\n    await this.fetchStockValues();\r\n  },\r\n  watch: {\r\n    \"route.params.symbol\"(newValue) {\r\n      this.stockInfo =\r\n        stocksArr.find((stock) => stock.Symbol === newValue) || {};\r\n      this.fetchStockValues();\r\n    },\r\n  },\r\n  methods: {\r\n    async fetchStockValues() {\r\n      const apiKey = \"TA4JH3O6GF8X4SLJ\";\r\n      const baseUrl = `https://www.alphavantage.co/query?function=TIME_SERIES_DAILY&symbol=${\r\n        this.symbol\r\n      }&apikey=${this.symbol === \"IBM\" ? \"demo\" : apiKey}`;\r\n      const response = await axios.get(baseUrl);\r\n      console.log(response);\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* Add your styles here */\r\n</style>\r\n"],"mappings":"AAyDA,SAASA,GAAG,EAAEC,QAAO,QAAS,KAAK;AACnC,SAASC,QAAO,QAAS,YAAY;AACrC,SAASC,MAAM,EAAEC,IAAG,QAAS