 route = useRoute();\r\n    const symbol = ref(route.params.symbol);\r\n    const companyInfo = ref(null);\r\n\r\n    onMounted(() => {\r\n      fetchCompanyInfo();\r\n    });\r\n\r\n    const fetchCompanyInfo = async () => {\r\n      try {\r\n        const apiKey = \"4L899V8QWQXTQP85\";\r\n        const response = await axios.get(\r\n          `https://www.alphavantage.co/query?function=OVERVIEW&symbol=${symbol.value}&apikey=${apiKey}`\r\n        );\r\n\r\n        if (response.status === 200) {\r\n          companyInfo.value = response.data;\r\n        }\r\n      } catch (error) {\r\n        console.error(\"Error fetching company information:\", error);\r\n      }\r\n    };\r\n\r\n    return {\r\n      symbol,\r\n      companyInfo,\r\n    };\r\n  },\r\n};\r\n</script>\r\n"],"mappings":"AAcA,OAAOA,UAAS,MAAO,6BAA6B;AACpD,SAASC,QAAO,QAAS,YAAY;AACrC,SAASC,GAAG,EAAEC,SAAQ,QAAS,KAAK;AACpC,OAAOC,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,UAAU,EAAE;IACVL;EACF,CAAC;EACDM,KAAKA,CAAA,EAAG;IACN,MAAMC,KAAI,GAAIN,QAAQ,CAAC,CAAC;IACxB,MAAMO,MAAK,GAAIN,GAAG,CAACK,KAAK,CAACE,MAAM,CAACD,MAAM,CAAC;IACvC,MAAME,WAAU,GAAIR,GAAG,CAAC,IAAI,CAAC;IAE7BC,SAAS,CAAC,MAAM;MACdQ,gBAAgB,CAAC,CAAC;IACpB,CAAC,CAAC;IAEF,MAAMA,gBAAe,GAAI,MAAAA,CAAA,KAAY;MACnC,IAAI;QACF,MAAMC,MAAK,GAAI,kBAAkB;QACjC,MAAMC,QAAO,GAAI,MAAMT,KAAK,CAACU,GAAG,CAC7B,8DAA6DN,MAAM,CAACO,KAAM,WAAUH,MAAO,EAC9F,CAAC;QAED,IAAIC,QAAQ,CAACG,MAAK,KAAM,GAAG,EAAE;UAC3BN,WAAW,CAACK,KAAI,GAAIF,QAAQ,CAACI,IAAI;QACnC;MACF,EAAE,OAAOC,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,qCAAqC,EAAEA,KAAK,CAAC;MAC7D;IACF,CAAC;IAED,OAAO;MACLV,MAAM;MACNE;IACF,CAAC;EACH;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}