{"ast":null,"code":"import { trendingDataExample } from \"@/store/trendingDataExample\";\nimport { computed, ref, watch } from \"vue\";\nexport default {\n  setup() {\n    const winners = trendingDataExample.top_gainers;\n    const losers = trendingDataExample.top_losers;\n    const popular = trendingDataExample.most_actively_traded;\n    const selectedType = ref(\"all\");\n    const filteredData = computed(() => {\n      if (selectedType.value === \"winners\") {\n        return winners;\n      } else if (selectedType.value === \"losers\") {\n        return losers;\n      } else if (selectedType.value === \"popular\") {\n        return popular;\n      } else {\n        // If selectedType is \"all\", combine all data\n        return [...winners, ...losers, ...popular];\n      }\n    });\n    watch(selectedType, () => {\n      filteredData.value = filteredData; // Force reactivity\n    });\n\n    return {\n      selectedType,\n      filteredData\n    };\n  }\n};","map":{"version":3,"names":["trendingDataExample","computed","ref","watch","setup","winners","top_gainers","losers","top_losers","popular","most_actively_traded","selectedType","filteredData","value"],"sources":["C:\\Users\\Edwaed\\Desktop\\stock_app\\fund-management-app\\src\\components\\TrendingStock.vue"],"sourcesContent":["<template>\r\n  <div class=\"trending-stocks\">\r\n    <a-radio-group v-model=\"selectedType\">\r\n      <a-radio-button value=\"all\">All</a-radio-button>\r\n      <a-radio-button value=\"winners\">Winners</a-radio-button>\r\n      <a-radio-button value=\"losers\">Losers</a-radio-button>\r\n      <a-radio-button value=\"popular\">Popular</a-radio-button>\r\n    </a-radio-group>\r\n\r\n    <a-table :data-source=\"filteredData\">\r\n      <a-table-column dataIndex=\"ticker\" />\r\n      <a-table-column dataIndex=\"price\" />\r\n      <a-table-column dataIndex=\"change_percentage\" />\r\n    </a-table>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { trendingDataExample } from \"@/store/trendingDataExample\";\r\nimport { computed, ref, watch } from \"vue\";\r\n\r\nexport default {\r\n  setup() {\r\n    const winners = trendingDataExample.top_gainers;\r\n    const losers = trendingDataExample.top_losers;\r\n    const popular = trendingDataExample.most_actively_traded;\r\n    const selectedType = ref(\"all\");\r\n\r\n    const filteredData = computed(() => {\r\n      if (selectedType.value === \"winners\") {\r\n        return winners;\r\n      } else if (selectedType.value === \"losers\") {\r\n        return losers;\r\n      } else if (selectedType.value === \"popular\") {\r\n        return popular;\r\n      } else {\r\n        // If selectedType is \"all\", combine all data\r\n        return [...winners, ...losers, ...popular];\r\n      }\r\n    });\r\n    watch(selectedType, () => {\r\n      filteredData.value = filteredData; // Force reactivity\r\n    });\r\n    return {\r\n      selectedType,\r\n      filteredData,\r\n    };\r\n  },\r\n};\r\n</script>\r\n"],"mappings":"AAkBA,SAASA,mBAAkB,QAAS,6BAA6B;AACjE,SAASC,QAAQ,EAAEC,GAAG,EAAEC,KAAI,QAAS,KAAK;AAE1C,eAAe;EACbC,KAAKA,CAAA,EAAG;IACN,MAAMC,OAAM,GAAIL,mBAAmB,CAACM,WAAW;IAC/C,MAAMC,MAAK,GAAIP,mBAAmB,CAACQ,UAAU;IAC7C,MAAMC,OAAM,GAAIT,mBAAmB,CAACU,oBAAoB;IACxD,MAAMC,YAAW,GAAIT,GAAG,CAAC,KAAK,CAAC;IAE/B,MAAMU,YAAW,GAAIX,QAAQ,CAAC,MAAM;MAClC,IAAIU,YAAY,CAACE,KAAI,KAAM,SAAS,EAAE;QACpC,OAAOR,OAAO;MAChB,OAAO,IAAIM,YAAY,CAACE,KAAI,KAAM,QAAQ,EAAE;QAC1C,OAAON,MAAM;MACf,OAAO,IAAII,YAAY,CAACE,KAAI,KAAM,SAAS,EAAE;QAC3C,OAAOJ,OAAO;MAChB,OAAO;QACL;QACA,OAAO,CAAC,GAAGJ,OAAO,EAAE,GAAGE,MAAM,EAAE,GAAGE,OAAO,CAAC;MAC5C;IACF,CAAC,CAAC;IACFN,KAAK,CAACQ,YAAY,EAAE,MAAM;MACxBC,YAAY,CAACC,KAAI,GAAID,YAAY,EAAE;IACrC,CAAC,CAAC;;IACF,OAAO;MACLD,YAAY;MACZC;IACF,CAAC;EACH;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}