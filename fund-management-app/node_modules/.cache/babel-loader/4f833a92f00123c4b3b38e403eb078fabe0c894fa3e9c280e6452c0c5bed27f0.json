{"ast":null,"code":"import { trendingDataExample } from \"@/store/trendingDataExample\";\nimport { computed, ref, watch, h } from \"vue\";\nimport { FallOutlined, RiseOutlined } from \"@ant-design/icons-vue\";\nimport { alphaApi } from \"@/services/alphaAPI\";\nexport default {\n  setup() {\n    const winners = ref({});\n    const losers = ref({});\n    const popular = ref({});\n    const columns = ref({});\n    const selectedType = ref(\"all\");\n    const filteredData = ref([]);\n    const loading = ref(true);\n    const fetchData = () => {\n      this.loading = true;\n      console.log(\"test\");\n      alphaApi.topGainersLosers().then(res => {\n        console.log(res);\n        winners.value = res.top_gainers;\n        losers.value = res.top_losers;\n        popular.value = res.most_actively_traded;\n        this.loading = false;\n      });\n    };\n    const updateFilteredData = () => {\n      if (selectedType.value === \"winners\") {\n        return winners.value;\n      } else if (selectedType.value === \"losers\") {\n        return losers.value;\n      } else if (selectedType.value === \"popular\") {\n        return popular.value;\n      } else {\n        console.log(winners.value);\n        // If selectedType is \"all\", combine all data\n        return [...winners.value, ...losers.value, ...popular.value];\n      }\n    };\n    filteredData.value = updateFilteredData();\n    watch(selectedType, () => {\n      filteredData.value = updateFilteredData(); // Force reactivity\n    });\n\n    columns.value = computed(() => {\n      return [{\n        title: \"Stock\",\n        dataIndex: \"ticker\",\n        key: \"ticker\"\n      }, {\n        title: \"Price Change\",\n        dataIndex: \"change_percentage\",\n        key: \"change_percentage\",\n        customRender: ({\n          text,\n          record\n        }) => {\n          const color = record.change_percentage.includes(\"-\") ? \"red\" : \"green\";\n          return record.change_percentage.includes(\"-\") ? [h(FallOutlined, {\n            style: {\n              color\n            }\n          }), h(\"span\", {\n            style: {\n              color,\n              marginLeft: \"10px\",\n              fontWeight: \"bold\"\n            }\n          }, text)] : [h(RiseOutlined, {\n            style: {\n              color\n            }\n          }), h(\"span\", {\n            style: {\n              color,\n              marginLeft: \"5px\",\n              fontWeight: \"bold\"\n            }\n          }, text)];\n        }\n      }, {\n        title: \"Price\",\n        dataIndex: \"price\",\n        key: \"price\",\n        customRender: ({\n          text\n        }) => {\n          return h(\"span\", {\n            style: {\n              fontWeight: \"bold\",\n              fontFamily: \"Montserrat, sans-serif\"\n            }\n          }, `$ ${text}`);\n        }\n      }];\n    });\n    fetchData();\n    return {\n      selectedType,\n      filteredData,\n      columns,\n      loading\n    };\n  }\n};","map":{"version":3,"names":["trendingDataExample","computed","ref","watch","h","FallOutlined","RiseOutlined","alphaApi","setup","winners","losers","popular","columns","selectedType","filteredData","loading","fetchData","console","log","topGainersLosers","then","res","value","top_gainers","top_losers","most_actively_traded","updateFilteredData","title","dataIndex","key","customRender","text","record","color","change_percentage","includes","style","marginLeft","fontWeight","fontFamily"],"sources":["C:\\Users\\Edwaed\\Desktop\\stock_app\\fund-management-app\\src\\components\\TrendingStock.vue"],"sourcesContent":["<template>\r\n  <div class=\"trending-stocks\">\r\n    <div v-if=\"loading\" class=\"loading\"><a-spin size=\"large\" /></div>\r\n    <div v-else>\r\n      <a-card>\r\n        <a-row justify=\"space-between\">\r\n          <a-col><h2>Trending Stock</h2></a-col>\r\n          <a-col>\r\n            <a-radio-group v-model:value=\"selectedType\">\r\n              <a-radio-button value=\"all\">All</a-radio-button>\r\n              <a-radio-button value=\"winners\">Winners</a-radio-button>\r\n              <a-radio-button value=\"losers\">Losers</a-radio-button>\r\n              <a-radio-button value=\"popular\">Popular</a-radio-button>\r\n            </a-radio-group>\r\n          </a-col>\r\n        </a-row>\r\n        <a-table\r\n          :data-source=\"filteredData\"\r\n          :showHeader=\"false\"\r\n          :columns=\"columns\"\r\n        />\r\n      </a-card>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { trendingDataExample } from \"@/store/trendingDataExample\";\r\nimport { computed, ref, watch, h } from \"vue\";\r\nimport { FallOutlined, RiseOutlined } from \"@ant-design/icons-vue\";\r\nimport { alphaApi } from \"@/services/alphaAPI\";\r\n\r\nexport default {\r\n  setup() {\r\n    const winners = ref({});\r\n    const losers = ref({});\r\n    const popular = ref({});\r\n    const columns = ref({});\r\n    const selectedType = ref(\"all\");\r\n    const filteredData = ref([]);\r\n    const loading = ref(true);\r\n    const fetchData = () => {\r\n      this.loading = true;\r\n      console.log(\"test\");\r\n      alphaApi.topGainersLosers().then((res) => {\r\n        console.log(res);\r\n        winners.value = res.top_gainers;\r\n        losers.value = res.top_losers;\r\n        popular.value = res.most_actively_traded;\r\n        this.loading = false;\r\n      });\r\n    };\r\n\r\n    const updateFilteredData = () => {\r\n      if (selectedType.value === \"winners\") {\r\n        return winners.value;\r\n      } else if (selectedType.value === \"losers\") {\r\n        return losers.value;\r\n      } else if (selectedType.value === \"popular\") {\r\n        return popular.value;\r\n      } else {\r\n        console.log(winners.value);\r\n        // If selectedType is \"all\", combine all data\r\n        return [...winners.value, ...losers.value, ...popular.value];\r\n      }\r\n    };\r\n    filteredData.value = updateFilteredData();\r\n\r\n    watch(selectedType, () => {\r\n      filteredData.value = updateFilteredData(); // Force reactivity\r\n    });\r\n\r\n    columns.value = computed(() => {\r\n      return [\r\n        {\r\n          title: \"Stock\",\r\n          dataIndex: \"ticker\",\r\n          key: \"ticker\",\r\n        },\r\n        {\r\n          title: \"Price Change\",\r\n          dataIndex: \"change_percentage\",\r\n          key: \"change_percentage\",\r\n          customRender: ({ text, record }) => {\r\n            const color = record.change_percentage.includes(\"-\")\r\n              ? \"red\"\r\n              : \"green\";\r\n            return record.change_percentage.includes(\"-\")\r\n              ? [\r\n                  h(FallOutlined, { style: { color } }),\r\n                  h(\r\n                    \"span\",\r\n                    {\r\n                      style: { color, marginLeft: \"10px\", fontWeight: \"bold\" },\r\n                    },\r\n                    text\r\n                  ),\r\n                ]\r\n              : [\r\n                  h(RiseOutlined, { style: { color } }),\r\n                  h(\r\n                    \"span\",\r\n                    {\r\n                      style: { color, marginLeft: \"5px\", fontWeight: \"bold\" },\r\n                    },\r\n                    text\r\n                  ),\r\n                ];\r\n          },\r\n        },\r\n        {\r\n          title: \"Price\",\r\n          dataIndex: \"price\",\r\n          key: \"price\",\r\n          customRender: ({ text }) => {\r\n            return h(\r\n              \"span\",\r\n              {\r\n                style: {\r\n                  fontWeight: \"bold\",\r\n                  fontFamily: \"Montserrat, sans-serif\",\r\n                },\r\n              },\r\n              `$ ${text}`\r\n            );\r\n          },\r\n        },\r\n      ];\r\n    });\r\n    fetchData();\r\n    return {\r\n      selectedType,\r\n      filteredData,\r\n      columns,\r\n      loading,\r\n    };\r\n  },\r\n};\r\n</script>\r\n"],"mappings":"AA2BA,SAASA,mBAAkB,QAAS,6BAA6B;AACjE,SAASC,QAAQ,EAAEC,GAAG,EAAEC,KAAK,EAAEC,CAAA,QAAS,KAAK;AAC7C,SAASC,YAAY,EAAEC,YAAW,QAAS,uBAAuB;AAClE,SAASC,QAAO,QAAS,qBAAqB;AAE9C,eAAe;EACbC,KAAKA,CAAA,EAAG;IACN,MAAMC,OAAM,GAAIP,GAAG,CAAC,CAAC,CAAC,CAAC;IACvB,MAAMQ,MAAK,GAAIR,GAAG,CAAC,CAAC,CAAC,CAAC;IACtB,MAAMS,OAAM,GAAIT,GAAG,CAAC,CAAC,CAAC,CAAC;IACvB,MAAMU,OAAM,GAAIV,GAAG,CAAC,CAAC,CAAC,CAAC;IACvB,MAAMW,YAAW,GAAIX,GAAG,CAAC,KAAK,CAAC;IAC/B,MAAMY,YAAW,GAAIZ,GAAG,CAAC,EAAE,CAAC;IAC5B,MAAMa,OAAM,GAAIb,GAAG,CAAC,IAAI,CAAC;IACzB,MAAMc,SAAQ,GAAIA,CAAA,KAAM;MACtB,IAAI,CAACD,OAAM,GAAI,IAAI;MACnBE,OAAO,CAACC,GAAG,CAAC,MAAM,CAAC;MACnBX,QAAQ,CAACY,gBAAgB,CAAC,CAAC,CAACC,IAAI,CAAEC,GAAG,IAAK;QACxCJ,OAAO,CAACC,GAAG,CAACG,GAAG,CAAC;QAChBZ,OAAO,CAACa,KAAI,GAAID,GAAG,CAACE,WAAW;QAC/Bb,MAAM,CAACY,KAAI,GAAID,GAAG,CAACG,UAAU;QAC7Bb,OAAO,CAACW,KAAI,GAAID,GAAG,CAACI,oBAAoB;QACxC,IAAI,CAACV,OAAM,GAAI,KAAK;MACtB,CAAC,CAAC;IACJ,CAAC;IAED,MAAMW,kBAAiB,GAAIA,CAAA,KAAM;MAC/B,IAAIb,YAAY,CAACS,KAAI,KAAM,SAAS,EAAE;QACpC,OAAOb,OAAO,CAACa,KAAK;MACtB,OAAO,IAAIT,YAAY,CAACS,KAAI,KAAM,QAAQ,EAAE;QAC1C,OAAOZ,MAAM,CAACY,KAAK;MACrB,OAAO,IAAIT,YAAY,CAACS,KAAI,KAAM,SAAS,EAAE;QAC3C,OAAOX,OAAO,CAACW,KAAK;MACtB,OAAO;QACLL,OAAO,CAACC,GAAG,CAACT,OAAO,CAACa,KAAK,CAAC;QAC1B;QACA,OAAO,CAAC,GAAGb,OAAO,CAACa,KAAK,EAAE,GAAGZ,MAAM,CAACY,KAAK,EAAE,GAAGX,OAAO,CAACW,KAAK,CAAC;MAC9D;IACF,CAAC;IACDR,YAAY,CAACQ,KAAI,GAAII,kBAAkB,CAAC,CAAC;IAEzCvB,KAAK,CAACU,YAAY,EAAE,MAAM;MACxBC,YAAY,CAACQ,KAAI,GAAII,kBAAkB,CAAC,CAAC,EAAE;IAC7C,CAAC,CAAC;;IAEFd,OAAO,CAACU,KAAI,GAAIrB,QAAQ,CAAC,MAAM;MAC7B,OAAO,CACL;QACE0B,KAAK,EAAE,OAAO;QACdC,SAAS,EAAE,QAAQ;QACnBC,GAAG,EAAE;MACP,CAAC,EACD;QACEF,KAAK,EAAE,cAAc;QACrBC,SAAS,EAAE,mBAAmB;QAC9BC,GAAG,EAAE,mBAAmB;QACxBC,YAAY,EAAEA,CAAC;UAAEC,IAAI;UAAEC;QAAO,CAAC,KAAK;UAClC,MAAMC,KAAI,GAAID,MAAM,CAACE,iBAAiB,CAACC,QAAQ,CAAC,GAAG,IAC/C,KAAI,GACJ,OAAO;UACX,OAAOH,MAAM,C