{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { useRouter } from \"vue-router\";\nimport { defineComponent, ref, h, watch, onMounted } from \"vue\";\nimport LayoutLogo from \"./LayoutLogo.vue\";\nexport default defineComponent({\n  components: {\n    LayoutLogo\n  },\n  setup() {\n    const router = useRouter();\n    const routes = router.options.routes.filter(route => route.meta);\n    const searchQuery = ref(\"\");\n    const selectedKeys = ref([]);\n    const navigate = routeName => {\n      router.push({\n        name: routeName\n      });\n    };\n    const handleSearch = () => {\n      // Perform search logic based on the searchQuery value\n      console.log(\"Search query:\", searchQuery.value);\n    };\n    const renderIcon = iconType => {\n      console.log(h(iconType));\n      return h(iconType);\n    };\n\n    // Watch for route changes and update selectedKeys\n    const updateSelectedKeys = () => {\n      selectedKeys.value = routes.filter(route => route.path === router.currentRoute.value.path).map(route => route.path);\n    };\n\n    // Listen to route changes\n    watch(() => router.currentRoute.value, () => {\n      updateSelectedKeys();\n    });\n\n    // Initial selection based on the current route\n    onMounted(() => {\n      updateSelectedKeys();\n    });\n    return {\n      routes: routes,\n      selectedKeys,\n      navigate,\n      searchQuery,\n      handleSearch,\n      renderIcon\n    };\n  }\n});","map":{"version":3,"names":["useRouter","defineComponent","ref","h","watch","onMounted","LayoutLogo","components","setup","router","routes","options","filter","route","meta","searchQuery","selectedKeys","navigate","routeName","push","name","handleSearch","console","log","value","renderIcon","iconType","updateSelectedKeys","path","currentRoute","map"],"sources":["C:\\Users\\Edwaed\\Desktop\\stock_app\\fund-management-app\\src\\components\\AppLayout.vue"],"sourcesContent":["<template>\r\n  <a-layout style=\"height: 100vh\">\r\n    <a-layout-sider breakpoint=\"lg\" collapsed-width=\"0\">\r\n      <LayoutLogo />\r\n      <a-menu\r\n        v-model:selectedKeys=\"selectedKeys\"\r\n        mode=\"inline\"\r\n        theme=\"dark\"\r\n        style=\"text-align: center\"\r\n      >\r\n        <a-menu-item\r\n          v-for=\"route in routes\"\r\n          :key=\"route.path\"\r\n          @click=\"navigate(route.name)\"\r\n        >\r\n          <span style=\"font-size: 18px; text-transform: capitalize\">\r\n            {{ route.meta.name }}\r\n          </span>\r\n        </a-menu-item>\r\n      </a-menu>\r\n    </a-layout-sider>\r\n    <a-layout>\r\n      <a-layout-header\r\n        :style=\"{\r\n          background: '#fff',\r\n          padding: '0 16px',\r\n          display: 'flex',\r\n          alignItems: 'center',\r\n        }\"\r\n      >\r\n        <a-input-search\r\n          placeholder=\"Search Stock\"\r\n          v-model=\"searchQuery\"\r\n          @search=\"handleSearch\"\r\n          size=\"large\"\r\n          :style=\"{ width: '200px' }\"\r\n        />\r\n      </a-layout-header>\r\n      <a-layout-content :style=\"{ margin: '20px' }\">\r\n        <div\r\n          :style=\"{ padding: '20px', background: '#fff', minHeight: '360px' }\"\r\n        >\r\n          <router-view />\r\n        </div>\r\n      </a-layout-content>\r\n    </a-layout>\r\n  </a-layout>\r\n</template>\r\n\r\n<script>\r\nimport { useRouter } from \"vue-router\";\r\nimport { defineComponent, ref, h, watch, onMounted } from \"vue\";\r\nimport LayoutLogo from \"./LayoutLogo.vue\";\r\n\r\nexport default defineComponent({\r\n  components: {\r\n    LayoutLogo,\r\n  },\r\n  setup() {\r\n    const router = useRouter();\r\n    const routes = router.options.routes.filter((route) => route.meta);\r\n    const searchQuery = ref(\"\");\r\n    const selectedKeys = ref([]);\r\n\r\n    const navigate = (routeName) => {\r\n      router.push({ name: routeName });\r\n    };\r\n\r\n    const handleSearch = () => {\r\n      // Perform search logic based on the searchQuery value\r\n      console.log(\"Search query:\", searchQuery.value);\r\n    };\r\n\r\n    const renderIcon = (iconType) => {\r\n      console.log(h(iconType));\r\n      return h(iconType);\r\n    };\r\n\r\n    // Watch for route changes and update selectedKeys\r\n    const updateSelectedKeys = () => {\r\n      selectedKeys.value = routes\r\n        .filter((route) => route.path === router.currentRoute.value.path)\r\n        .map((route) => route.path);\r\n    };\r\n\r\n    // Listen to route changes\r\n    watch(\r\n      () => router.currentRoute.value,\r\n      () => {\r\n        updateSelectedKeys();\r\n      }\r\n    );\r\n\r\n    // Initial selection based on the current route\r\n    onMounted(() => {\r\n      updateSelectedKeys();\r\n    });\r\n\r\n    return {\r\n      routes: routes,\r\n      selectedKeys,\r\n      navigate,\r\n      searchQuery,\r\n      handleSearch,\r\n      renderIcon,\r\n    };\r\n  },\r\n});\r\n</script>\r\n"],"mappings":";AAkDA,SAASA,SAAQ,QAAS,YAAY;AACtC,SAASC,eAAe,EAAEC,GAAG,EAAEC,CAAC,EAAEC,KAAK,EAAEC,SAAQ,QAAS,KAAK;AAC/D,OAAOC,UAAS,MAAO,kBAAkB;AAEzC,eAAeL,eAAe,CAAC;EAC7BM,UAAU,EAAE;IACVD;EACF,CAAC;EACDE,KAAKA,CAAA,EAAG;IACN,MAAMC,MAAK,GAAIT,SAAS,CAAC,CAAC;IAC1B,MAAMU,MAAK,GAAID,MAAM,CAACE,OAAO,CAACD,MAAM,CAACE,MAAM,CAAEC,KAAK,IAAKA,KAAK,CAACC,IAAI,CAAC;IAClE,MAAMC,WAAU,GAAIb,GAAG,CAAC,EAAE,CAAC;IAC3B,MAAMc,YAAW,GAAId,GAAG,CAAC,EAAE,CAAC;IAE5B,MAAMe,QAAO,GAAKC,SAAS,IAAK;MAC9BT,MAAM,CAACU,IAAI,CAAC;QAAEC,IAAI,EAAEF;MAAU,CAAC,CAAC;IAClC,CAAC;IAED,MAAMG,YAAW,GAAIA,CAAA,KAAM;MACzB;MACAC,OAAO,CAACC,GAAG,CAAC,eAAe,EAAER,WAAW,CAACS,KAAK,CAAC;IACjD,CAAC;IAED,MAAMC,UAAS,GAAKC,QAAQ,IAAK;MAC/BJ,OAAO,CAACC,GAAG,CAACpB,CAAC,CAACuB,QAAQ,CAAC,CAAC;MACxB,OAAOvB,CAAC,CAACuB,QAAQ,CAAC;IACpB,CAAC;;IAED;IACA,MAAMC,kBAAiB,GAAIA,CAAA,KAAM;MAC/BX,YAAY,CAACQ,KAAI,GAAId,MAAK,CACvBE,MAAM,CAAEC,KAAK,IAAKA,KAAK,CAACe,IAAG,KAAMnB,MAAM,CAACoB,YAAY,CAACL,KAAK,CAACI,IAAI,EAC/DE,GAAG,CAAEjB,KAAK,IAAKA,KAAK,CAACe,