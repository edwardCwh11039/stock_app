{"ast":null,"code":"// import { apiService } from \"@/services/fundAPI\";\nimport { AButton, ATable } from \"ant-design-vue\";\nimport { computed, ref } from \"vue\";\nimport { stocksArr } from \"@/store/stockArr\";\nexport default {\n  components: {\n    AButton,\n    ATable\n  },\n  setup() {\n    const filteredInfo = ref();\n    const sortedInfo = ref();\n    const columns = computed(() => {\n      const sorted = sortedInfo.value || {};\n      return [{\n        title: \"Symbol\",\n        dataIndex: \"Symbol\",\n        key: \"Symbol\",\n        sorter: (a, b) => a.Symbol.localeCompare(b.Symbol),\n        sortOrder: sorted.columnKey === \"Symbol\" && sorted.order,\n        ellipsis: true\n      }, {\n        title: \"Company Name\",\n        dataIndex: \"CompanyName\",\n        key: \"CompanyName\",\n        sorter: (a, b) => a.CompanyName.localeCompare(b.CompanyName),\n        sortOrder: sorted.columnKey === \"CompanyName\" && sorted.order,\n        ellipsis: true\n      }, {\n        title: \"Price Change\",\n        dataIndex: \"PriceChange\",\n        key: \"PriceChange\",\n        sorter: (a, b) => a.PriceChange.localeCompare(b.PriceChange),\n        sortOrder: sorted.columnKey === \"PriceChange\" && sorted.order,\n        ellipsis: true\n      }, {\n        title: \"Price\",\n        dataIndex: \"Price\",\n        key: \"Price\",\n        sorter: (a, b) => a.Price - b.Price,\n        sortOrder: sorted.columnKey === \"Price\" && sorted.order\n      }];\n    });\n    const handleChange = (pagination, filters, sorter) => {\n      console.log(\"Various parameters\", pagination, filters, sorter);\n      filteredInfo.value = filters;\n      sortedInfo.value = sorter;\n    };\n    const clearFilters = () => {\n      filteredInfo.value = null;\n    };\n    const clearAll = () => {\n      filteredInfo.value = null;\n      sortedInfo.value = null;\n    };\n    const setPriceSort = () => {\n      sortedInfo.value = {\n        order: \"descend\",\n        columnKey: \