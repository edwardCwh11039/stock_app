ll\r\n          </a-button>\r\n        </a-col>\r\n      </a-row>\r\n    </a-col>\r\n    <a-modal\r\n      v-model:open=\"isModalVisible\"\r\n      :title=\"modalTitle\"\r\n      ok-text=\"Confirm\"\r\n      cancel-text=\"Cancel\"\r\n      @ok=\"handleModalConfirm\"\r\n    >\r\n      <p>Amount:</p>\r\n      <a-slider\r\n        v-model:value=\"amountValue\"\r\n        :tooltip-open=\"true\"\r\n        :tip-formatter=\"formatter\"\r\n      />\r\n      <p>Stock:</p>\r\n      <a-input-number id=\"inputNumber\" v-model:value=\"stockValue\" :min=\"0\">\r\n        <template #upIcon>\r\n          <ArrowUpOutlined />\r\n        </template>\r\n        <template #downIcon>\r\n          <ArrowDownOutlined />\r\n        </template>\r\n      </a-input-number>\r\n    </a-modal>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport StockChart from \"@/components/StockChart.vue\";\r\nimport { useRoute } from \"vue-router\";\r\nimport { ref } from \"vue\";\r\nimport { stocksArr } from \"@/store/stockArr\";\r\nimport {\r\n  FallOutlined,\r\n  RiseOutlined,\r\n  ArrowUpOutlined,\r\n  ArrowDownOutlined,\r\n} from \"@ant-design/icons-vue\";\r\n\r\nexport default {\r\n  components: {\r\n    StockChart,\r\n    FallOutlined,\r\n    RiseOutlined,\r\n    ArrowUpOutlined,\r\n    ArrowDownOutlined,\r\n  },\r\n  setup() {\r\n    const route = useRoute();\r\n    const symbol = ref(route.params.symbol);\r\n    // console.log(symbol, stocksArr);\r\n    const companyInfo = stocksArr.find(\r\n      (stock) => stock.Symbol === symbol.value\r\n    );\r\n    const priceChangeClass = companyInfo.PriceChange.includes(\"-\")\r\n      ? \"negative-price\"\r\n      : \"positive-price\";\r\n    const isModalVisible = ref(false);\r\n    const modalTitle = ref(\"\");\r\n    const amount = ref(1);\r\n    const stock = ref(\"\");\r\n    const stockValue = ref(0);\r\n    const showModal = (action) => {\r\n      stock.value = \"\"; // Clear previous input\r\n      isModalVisible.value = true;\r\n      modalTitle.value = (action === \"buy\" ? \"Buy\" : \"Sell\").concat(\r\n        \" \",\r\n        symbol.value,\r\n        \" Stock\"\r\n      );\r\n    };\r\n    const handleModalConfirm = () => {\r\n      // Implement the logic for handling the modal confirm here\r\n      // You can access the selected amount and stock using the `amount` and `stock` refs\r\n      // After handling, close the modal using `isModalVisible.value = false;`\r\n      isModalVisible.value = false;\r\n    };\r\n    const amountValue = ref(30);\r\n    const formatter = (value) => {\r\n      return `$${value}`;\r\n    };\r\n    return {\r\n      symbol,\r\n      companyInfo,\r\n      priceChangeClass,\r\n      isModalVisible,\r\n      modalTitle,\r\n      amount,\r\n      stock,\r\n      showModal,\r\n      handleModalConfirm,\r\n      amountValue,\r\n      formatter,\r\n      stockValue,\r\n    };\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.stock-detail {\r\n  max-height: 75vh;\r\n  overflow: auto;\r\n}\r\n\r\n.logo {\r\n  height: 50px;\r\n  width: 50px;\r\n  max-width: 100px;\r\n  margin-right: 20px;\r\n}\r\n\r\n.price {\r\n  font-size: 1.2rem;\r\n  margin-bottom: 5px;\r\n}\r\n\r\n.positive-price {\r\n  color: green;\r\n  font-weight: bold;\r\n  font-size: 1.2rem;\r\n}\r\n\r\n.negative-price {\r\n  color: red;\r\n  font-size: 1.2rem;\r\n  font-weight: bold;\r\n}\r\n</style>\r\n"],"mappings":"AAkFA,OAAOA,UAAS,MAAO,6BAA6B;AACpD,SAASC,QAAO,QAAS,YAAY;AACrC,SAASC,GAAE,QAAS,KAAK;AACzB,SAASC,SAAQ,QAAS,kBAAkB;AAC5C,SACEC,YAAY,EACZC,YAAY,EACZC,eAAe,EACfC,iBAAiB,QACZ,uBAAuB;AAE9B,eAAe;EACbC,UAAU,EAAE;IACVR,UAAU;IACVI,YAAY;IACZC,YAAY;IACZC,eAAe;IACfC;EACF,CAAC;EACDE,KAAKA,CAAA,EAAG;IACN,MAAMC,KAAI,GAAIT,QAAQ,CAAC,CAAC;IACxB,MAAMU,MAAK,GAAIT,GAAG,CAACQ,KAAK,CAACE,MAAM,CAACD,MAAM,CAAC;IACvC;IACA,MAAME,WAAU,GAAIV,SAAS,CAACW,IAAI,CAC/BC,KAAK,IAAKA,KAAK,CAACC,MAAK,KAAML,MAAM,CAACM,KACrC,CAAC;IACD,MAAMC,gBAAe,GAAIL,WAAW,CAACM,WAAW,CAACC,QAAQ,CAAC,GAAG,IACzD,gBAAe,GACf,gBAAgB;IACpB,MAAMC,cAAa,GAAInB,GAAG,CAAC,K