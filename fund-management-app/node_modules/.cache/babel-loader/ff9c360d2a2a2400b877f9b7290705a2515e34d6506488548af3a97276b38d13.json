{"ast":null,"code":"import StockChart from \"@/components/StockChart.vue\";\nimport { useRoute } from \"vue-router\";\nimport { ref } from \"vue\";\nimport { stocksArr } from \"@/store/stockArr\";\nimport { FallOutlined, RiseOutlined, ArrowUpOutlined, ArrowDownOutlined } from \"@ant-design/icons-vue\";\nexport default {\n  components: {\n    StockChart,\n    FallOutlined,\n    RiseOutlined,\n    ArrowUpOutlined,\n    ArrowDownOutlined\n  },\n  setup() {\n    const route = useRoute();\n    const symbol = ref(route.params.symbol);\n    // console.log(symbol, stocksArr);\n    const companyInfo = stocksArr.find(stock => stock.Symbol === symbol.value);\n    const priceChangeClass = companyInfo.PriceChange.includes(\"-\") ? \"negative-price\" : \"positive-price\";\n    const isModalVisible = ref(false);\n    const modalTitle = ref(\"\");\n    const amount = ref(1);\n    const stock = ref(\"\");\n    const stockValue = ref(0);\n    const showModal = action => {\n      stock.value = \"\"; // Clear previous input\n      isModalVisible.value = true;\n      modalTitle.value = (action === \"buy\" ? \"Buy\" : \"Sell\").concat(\" \", symbol.value, \" Stock\");\n    };\n    const handleModalConfirm = () => {\n      // Implement the logic for handling the modal confirm here\n      // You can access the selected amount and stock using the `amount` and `stock` refs\n      // After handling, close the modal using `isModalVisible.value = false;`\n      isModalVisible.value = false;\n    };\n    const amountValue = ref(30);\n    const formatter = value => {\n      return `$${value}`;\n    };\n    return {\n      symbol,\n      companyInfo,\n      priceChangeClass,\n      isModalVisible,\n      modalTitle,\n      amount,\n      stock,\n      showModal,\n      handleModalConfirm,\n      amountValue,\n      formatter,\n      stockValue\n    };\n  }\n};","map":{"version":3,"names":["StockChart","useRoute","ref","stocksArr","FallOutlined","RiseOutlined","ArrowUpOutlined","ArrowDownOutlined",