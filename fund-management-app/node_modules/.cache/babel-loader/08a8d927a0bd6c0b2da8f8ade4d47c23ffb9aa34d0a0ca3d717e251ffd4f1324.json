nst priceChangeClass = companyInfo.PriceChange.includes(\"-\")\r\n      ? \"negative-price\"\r\n      : \"positive-price\";\r\n    const isModalVisible = ref(false);\r\n    const modalTitle = ref(\"\");\r\n    const amount = ref(1);\r\n    const stock = ref(\"\");\r\n    const stockValue = ref(0);\r\n    const showModal = (action) => {\r\n      stock.value = \"\"; // Clear previous input\r\n      isModalVisible.value = true;\r\n      modalTitle.value = (action === \"buy\" ? \"Buy\" : \"Sell\").concat(\r\n        \" \",\r\n        symbol.value,\r\n        \" Stock\"\r\n      );\r\n    };\r\n    const handleModalConfirm = () => {\r\n      isModalVisible.value = false;\r\n    };\r\n    const amountValue = ref(30);\r\n    const formatter = (value) => {\r\n      return `$${value}`;\r\n    };\r\n    const updateStockValueFromSlider = (value) => {\r\n      stockValue.value = value;\r\n    };\r\n    const updateSliderkValueFromInput = (value) => {\r\n      amountValue.value = value;\r\n    };\r\n\r\n    const getPrice = (price) => {\r\n      console.log(price)\r\n      currentPrice.value = price[0];\r\n      console.log(currentPrice)\r\n    };\r\n    return {\r\n      symbol,\r\n      companyInfo,\r\n      priceChangeClass,\r\n      isModalVisible,\r\n      modalTitle,\r\n      amount,\r\n      stock,\r\n      showModal,\r\n      handleModalConfirm,\r\n      amountValue,\r\n      formatter,\r\n      stockValue,\r\n      updateSliderkValueFromInput,\r\n      updateStockValueFromSlider,\r\n      getPrice,\r\n    };\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.stock-detail {\r\n  max-height: 75vh;\r\n  overflow: auto;\r\n}\r\n\r\n.logo {\r\n  height: 50px;\r\n  width: 50px;\r\n  max-width: 100px;\r\n  margin-right: 20px;\r\n}\r\n\r\n.price {\r\n  font-size: 1.2rem;\r\n  margin-bottom: 5px;\r\n}\r\n\r\n.positive-price {\r\n  color: green;\r\n  font-weight: bold;\r\n  font-size: 1.2rem;\r\n}\r\n\r\n.negative-price {\r\n  color: red;\r\n  font-size: 1.2rem;\r\n  font-weig